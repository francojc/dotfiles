#!/usr/bin/env bash
# Wrapper for personal-contacts-fetcher.py

SCRIPT_NAME="cntc-fetch"; SCRIPT_VERSION="1.0.0"
. "$(dirname "$0")/common.sh"

main(){
  if [[ -x "$(dirname "$0")/personal-contacts-fetcher.py" ]]; then
    "$(dirname "$0")/personal-contacts-fetcher.py" "$@"
  else
    die "personal-contacts-fetcher.py not found"
  fi
}

main "$@"
