#!/usr/bin/env bash
# Wrapper for setup-mcp-repos-gh.sh

SCRIPT_NAME="gh-mcp-setup"; SCRIPT_VERSION="1.0.0"
. "$(dirname "$0")/common"

main(){
  if [[ -x "$(dirname "$0")/setup-mcp-repos-gh.sh" ]]; then
    "$(dirname "$0")/setup-mcp-repos-gh.sh" "$@"
  else
    die "setup-mcp-repos-gh.sh not found"
  fi
}

main "$@"
